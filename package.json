{
  "name": "api-generator",
  "version": "0.1.0",
  "description": "This tool generates a Typescript and express based REST server API from specification file that conforms to a subset of the OpenAPI 3 specification.",
  "main": "dist/lib/main.js",
  "scripts": {
    "start": "node dist/lib/main.js",
    "prepare": "tsc",
    "build": "tsc"
  },
  "bin": {
    "api-generator": "./bin/api-generator"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BerryControl/api-generator.git"
  },
  "keywords": [
    "openapi",
    "generator",
    "typescript",
    "express.js"
  ],
  "author": "Thomas Bonk",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/BerryControl/api-generator/issues"
  },
  "homepage": "https://github.com/BerryControl/api-generator#readme",
  "dependencies": {
    "commander": "^11.1.0",
    "nunjucks": "^3.2.4",
    "semver": "^7.5.4",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/nunjucks": "^3.2.6",
    "@types/semver": "^7.5.6",
    "typescript": "^5.3.3"
  }
}
